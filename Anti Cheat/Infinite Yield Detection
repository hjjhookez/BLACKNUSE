if not game:IsLoaded() then
    game.Loaded:Wait()
end

task.wait(3)

local t = setmetatable({}, {__mode="v"})
while task.wait() do
    t[1] = {}
    t[2] = game:GetService("NetworkClient")
    while t[1] ~= nil do
        t[3] = string.rep("ab", 1024*2)
        t[3] = nil
        task.wait()
    end
    if t[2] ~= nil then
        game.Players.LocalPlayer:Kick("Detect")
        break
    end
end
